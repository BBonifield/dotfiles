[user]
  name = AUTHORNAME
  email = AUTHOREMAIL
[alias]
  aliases = config --get-regexp alias
  br = branch
  cleanlog = log --abbrev-commit --pretty=oneline
  cl = cleanlog -10
  co = checkout
  com = commit -v
  cp = cherry-pick
  d = diff
	rh = reset HEAD^
  unstage = reset HEAD
  s = status -s
[color]
  ui = true
[core]
  excludesfile = ~/.gitignore
  editor = /usr/bin/vim
  pager = less -F -X
[credential]
  helper = osxkeychain
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[merge]
  tool = vimdiff
[push]
  default = current
